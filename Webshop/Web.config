      <add name="AspNetStaticFileHandler-HTM" path="*.htm" verb="GET,HEAD" type="System.Web.StaticFileHandler" />
      <add name="ForbiddenFolderHandler" path="*/dc_files/*" verb="*" type="System.Web.HttpForbiddenHandler" preCondition="integratedMode" />
    </handlers>
    <security>
      <requestFiltering allowDoubleEscaping="true">
        <requestLimits maxAllowedContentLength="2147483648" maxQueryString="2048" maxUrl="4096" />
      </requestFiltering>
    </security>
    <httpErrors existingResponse="PassThrough" />
  </system.webServer>
